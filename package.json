  "bugs": {
    "url": "https://github.com/nicknamejv/ProjectJourneyJapan/issues"
  },
  "homepage": "https://github.com/nicknamejv/ProjectJourneyJapan#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.1",
    "concurrently": "^6.2.1",
    "connect-mongo": "^4.5.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "mongoose": "^6.0.5",
