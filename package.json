{
  "name": "projectjourneyjapan",
  "version": "1.0.0",
  "description": "Journey Japan is a web application catered to those who are thinking of traveling to Japan, been to Japan, or just genuinely interested about what Japan has to offer. Users will have the ability to create reviews on restaurants and attractions that they have been showcasing real experiences to give insight for other users about that restaurant or attraction.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "watch-sass": "sass --watch sass/src:public/styles",
    "watch-nodemon": "nodemon server.js",
    "dev": "open http://localhost:4000/journeyjapan && concurrently -n sass,server -c 'bgGreen.bold,bgBlue.bold' 'npm:watch-sass' 'npm:watch-nodemon'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicknamejv/ProjectJourneyJapan.git"
  },
  "author": "Jon Ver Cabral Dela Cruz",
